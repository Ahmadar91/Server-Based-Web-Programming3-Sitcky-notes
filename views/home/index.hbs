
<div class="container">
  <!-- Example row of columns -->
  <div class="row">
    {{#if viewData.codeSnippets.length }}
    {{#each viewData.codeSnippets}}
    <div class="col-md-6">
            <h3  class="h3">
        Title:{{this.title}}
      </h3>
      <pre>
      <code>
{{this.description}}
      </code>
      </pre>
      <small>created by {{this.name}}</small>
    </div>
  {{/each}}
    </div>
  {{else}}
  <p> No snippets to display.</p>
  {{/if}}